Package: splinkr
Type: Package
Title: Connect R Enviroment to SpeciesLink Network
Version: 0.1.0
Authors@R: person(given = "Carlos Eduardo", 
                   family = "Siqueira",
                   role = c("aut", "cre"),
                   email = "caduarte7@gmail.com",
                   comment = c(ORCID = "0000-0003-4232-8082")
